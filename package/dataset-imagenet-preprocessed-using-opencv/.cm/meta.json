{
  "check_exit_status": "yes",
  "customize": {
    "install_env": {
      "CK_IMAGE_FILE": "",
      "PACKAGE_VERSION": "2012",
      "_SUBSET_FOF": "image_list.txt",
      "_SUBSET_OFFSET": "0",
      "_SUBSET_VOLUME": "500"
    },
    "soft_uoa": "dataset.imagenet.preprocessed",
    "no_os_in_suggested_path": "yes",
    "no_ver_in_suggested_path": "yes",
    "skip_file_check": "yes"
  },
  "deps": {
    "dataset-source": {
      "enable_if_env": [
        {
          "CK_IMAGE_FILE": [
            ""
          ]
        }
      ],
      "local": "yes",
      "name": "ImageNet dataset (original or calibration)",
      "sort": 10,
      "tags": "dataset,imagenet",
      "or_tags": "original;calibration",
      "update_tags_if_env": {
        "full": [
          {
            "_SUBSET_VOLUME": "50000"
          }
        ]
      }
    },
    "lib-python-numpy": {
      "local": "yes",
      "name": "Python NumPy library",
      "sort": 110,
      "tags": "lib,python-package,numpy"
    },
    "lib-python-cv2": {
      "local": "yes",
      "name": "Python OpenCV library",
      "sort": 120,
      "tags": "lib,python-package,cv2"
    },
    "python": {
      "local": "yes",
      "name": "Python interpreter",
      "sort": 100,
      "tags": "compiler,lang-python"
    }
  },
  "end_full_path_universal": "ILSVRC2012_val_00000001.JPEG",
  "only_for_host_os_tags": [],
  "only_for_target_os_tags": [],
  "process_script": "shell_wrapper",
  "suggested_path": "dataset-imagenet-preprocessed-using-opencv",
  "tags": [
    "image-classification",
    "dataset",
    "imagenet",
    "2012",
    "val",
    "preprocessed",
    "using-opencv"
  ],
  "variations": {
    "universal": {
      "on_by_default": "yes",
      "extra_env": {
        "_DATA_TYPE": "uint8",
        "_NEW_EXTENSION": "rgb8",
        "_NORMALIZE_DATA": "0",
        "_SUBTRACT_MEAN": "0",
        "_GIVEN_CHANNEL_MEANS": ""
      },
      "extra_customize": {
        "soft_uoa": "dataset.imagenet.preprocessed"
      }
    },
    "for.mobilenet": {
      "extra_env": {
        "_DATA_TYPE": "float32",
        "_NEW_EXTENSION": "rgbf32",
        "_NORMALIZE_DATA": "YES",
        "_SUBTRACT_MEAN": "0",
        "_GIVEN_CHANNEL_MEANS": "",
        "_INTERPOLATION_METHOD": "INTER_LINEAR"
      },
      "extra_customize": {
        "soft_uoa": "dataset.imagenet.preprocessed.rgbf32"
      }
    },
    "for.resnet": {
      "extra_env": {
        "_DATA_TYPE": "float32",
        "_NEW_EXTENSION": "rgbf32",
        "_NORMALIZE_DATA": "0",
        "_SUBTRACT_MEAN": "YES",
        "_GIVEN_CHANNEL_MEANS": "123.68 116.78 103.94",
        "_INTERPOLATION_METHOD": "INTER_AREA"
      },
      "extra_customize": {
        "soft_uoa": "dataset.imagenet.preprocessed.rgbf32"
      }
    },
    "crop.1000": {
      "extra_env": {
        "_CROP_FACTOR": "100.0"
      }
    },
    "crop.875": {
      "on_by_default": "yes",
      "extra_env": {
        "_CROP_FACTOR": "87.5"
      }
    },
    "external_file": {
      "extra_customize": {
        "package_name": "Pre-processed single image ($<<CK_IMAGE_FILE>>$)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": "1"
      }
    },
    "first.1": {
      "extra_customize": {
        "package_extra_name": " (first 1)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": "1"
      }
    },
    "first.5": {
      "extra_customize": {
        "package_extra_name": " (first 5)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": "5"
      }
    },
    "first.20": {
      "extra_customize": {
        "package_extra_name": " (first 20)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": "20"
      }
    },
    "first.500": {
      "on_by_default": "yes",
      "extra_customize": {
        "package_extra_name": " (first 500)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": "500"
      }
    },
    "full": {
      "extra_customize": {
        "package_extra_name": " (full)"
      },
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": "50000"
      }
    },
    "side.96": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "96"
      }
    },
    "side.128": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "128"
      }
    },
    "side.160": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "160"
      }
    },
    "side.192": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "192"
      }
    },
    "side.224": {
      "on_by_default": "yes",
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "224"
      }
    },
    "side.240": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "240"
      }
    },
    "side.260": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "260"
      }
    },
    "side.280": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "280"
      }
    },
    "side.300": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "300"
      }
    },
    "layout.nhwc": {
      "on_by_default": "yes",
      "extra_env": {
        "_DATA_LAYOUT": "NHWC"
      }
    },
    "layout.nchw": {
      "extra_env": {
        "_DATA_LAYOUT": "NCHW"
      }
    },
    "inter.linear": {
      "on_by_default": "yes",
      "extra_env": {
        "_INTERPOLATION_METHOD": "INTER_LINEAR"
      }
    },
    "inter.area": {
      "extra_env": {
        "_INTERPOLATION_METHOD": "INTER_AREA"
      }
    }
  }
}
