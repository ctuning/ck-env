    "install_env": {
      "CMAKE_CONFIG": "Release",
      "PACKAGE_BUILD_TYPE": "cmake",
      "PACKAGE_CONFIGURE_FLAGS": "-DBENCHMARK_ENABLE_LTO=OFF -DBENCHMARK_ENABLE_TESTING=OFF -DBUILD_STATIC_LIBS=ON -DBUILD_SHARED_LIBS=OFF",
      "PACKAGE_CONFIGURE_FLAGS_ANDROID": "-DCMAKE_CROSSCOMPILING=1 -DRUN_HAVE_STD_REGEX=0 -DRUN_HAVE_POSIX_REGEX=0 -DRUN_HAVE_GNU_POSIX_REGEX=0 -DRUN_HAVE_STEADY_CLOCK=0",
      "PACKAGE_CONFIGURE_FLAGS_LINUX": "",
