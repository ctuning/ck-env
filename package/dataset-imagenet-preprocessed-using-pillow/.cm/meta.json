{
  "check_exit_status": "yes", 
  "customize": {
    "install_env": {
        "_INPUT_SQUARE_SIDE": "224",
        "_CROP_FACTOR": "87.5",
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": "500",
        "_SUBSET_FOF": "image_list.txt",
        "_NEW_EXTENSION": "rgb8",
        "_DATA_TYPE": "uint8",
        "PACKAGE_VERSION": "2012",
        "CK_IMAGE_FILE": ""
    }, 
    "no_os_in_suggested_path": "yes", 
    "no_ver_in_suggested_path": "yes", 
    "skip_file_check": "yes"
  }, 
  "deps": {
    "dataset-source": {
      "enable_if_env": [
        { "CK_IMAGE_FILE": [ "" ] }
      ],
      "local": "yes", 
      "name": "ImageNet dataset (original)",
      "sort": 10, 
      "tags": "dataset,imagenet,original",
      "update_tags_if_env": {
        "full": [
          {
            "_SUBSET_VOLUME": "50000"
          }
        ]
      }
    },
    "python": {
      "local": "yes",
      "name": "Python interpreter",
      "sort": 100,
      "tags": "compiler,lang-python"
    },
    "lib-python-numpy": {
      "local": "yes",
      "name": "Python NumPy library",
      "sort": 110,
      "tags": "lib,python-package,numpy"
    },
    "lib-python-pillow": {
      "local": "yes",
      "name": "Python Pillow library",
      "sort": 130,
      "tags": "lib,python-package,pillow"
    }
  }, 
  "end_full_path_universal": "ILSVRC2012_val_00000001.JPEG",
  "only_for_host_os_tags": [], 
  "only_for_target_os_tags": [], 
  "process_script": "shell_wrapper", 
  "soft_uoa": "dataset.imagenet.preprocessed", 
  "suggested_path": "dataset-imagenet-preprocessed-using-pillow",
  "tags": [
    "image-classification",
    "dataset", 
    "imagenet",
    "2012",
    "val",
    "preprocessed",
    "using-pillow"
  ], 
  "variations": {
    "external_file": {
      "extra_env": {
        "_SUBSET_OFFSET": "0",
        "_SUBSET_VOLUME": "1"
      },
      "extra_customize": {
        "package_name": "Pre-processed single image ($<<CK_IMAGE_FILE>>$)"
      }
    },
    "crop.875": {
      "extra_env": {
        "_CROP_FACTOR": "87.5"
      }
    }, 
    "crop.1000": {
      "extra_env": {
        "_CROP_FACTOR": "100.0"
      }
    },
    "side.300": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "300"
      }
    },
    "side.280": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "280"
      }
    },
    "side.260": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "260"
      }
    },
    "side.256": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "256"
      }
    },
    "side.240": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "240"
      }
    },
    "side.224": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "224"
      }
    },
    "side.192": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "192"
      }
    },
    "side.160": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "160"
      }
    },
    "side.128": {
      "extra_env": {
        "_INPUT_SQUARE_SIDE": "128"
