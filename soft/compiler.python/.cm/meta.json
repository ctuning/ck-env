{
  "auto_detect": "yes",
  "customize": {
    "add_only_with_version": "yes",
    "build_dir_name": "python",
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_COMPILER_PYTHON",
    "languages": [
      "PYTHON"
    ],
    "limit_recursion_dir_search": {
      "linux": 3,
      "win": 4
    },
    "only_for_host_os_tags": [
      "windows",
      "linux"
    ],
    "only_for_target_os_tags": [
      "windows",
      "android",
      "linux"
    ],
    "soft_file": {
      "linux": "python*",
      "win": "python*.exe"
    },
    "soft_file_from_host_os": "yes",
    "soft_version_cmd": {
      "linux": "--version > $#filename#$ 2>&1",
      "win": "--version > $#filename#$ 2>&1"
    }
  },
  "env": {
    "CK_PYC": "python",
    "CK_PY_OBJ_EXT": ".pyc",
    "CK_PY_RT": "python"
  },
  "soft_name": "python",
  "tags": [
    "compiler",
    "python",
    "lang-python"
  ],
  "template": "yes",
  "template_type": "python"
}
